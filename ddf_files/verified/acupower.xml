      </value>
      <valueInc>1</valueInc>
      <label><getRowOid>.1.3.6.1.4.1.6302.2.1.2.7.3.1.3</getRowOid></label>
      <sensorSet>Temperature</sensorSet>
    </numSensor>
	
	<stateSensor ruleid="temperature_type" index=".1.3.6.1.4.1.6302.2.1.2.7.3.1.4">
