      </value>
	  <valueInc>0.001</valueInc>
      <label>PS Battery Current</label>
      <sensorSet>psBattery</sensorSet>
    </numSensor>
	
    <numSensor ruleid="battery_capacity">
	  <rowTest>
		<mapValue mapid="deleteNull-vm">
			<getOid>.1.3.6.1.4.1.6302.2.1.2.5.3.0</getOid>
		</mapValue>
      </rowTest>
      <type>pctofcapacity</type>
      <sensorId>_DC_RECTIFIER_BATT_CAPACITY_1</sensorId>
      <value>
		<mult>
			<op>
				<getOid>.1.3.6.1.4.1.6302.2.1.2.5.3.0</getOid>
			</op>
			<op>0.001</op>
		</mult>
      </value>
      <valueInc>0.001</valueInc>
