<?xml version="1.0"?>
<!-- ACU-Power -->
<APC_DDF
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="apc_ddf_1_6.xsd"
	ddfid="acupower"
	ddfname="ACU-Power"
	ddfversion="9"
	canremove="yes">
  <schemaVersion>1.6</schemaVersion>

<!--ACU Ident -->

	<device deviceid="acupowerdiscdata">
    <oidMustExist ruleid="mib2objidtest" oid=".1.3.6.1.4.1.6302.2.1" istree="yes"/>

 		<!--vendor -->
	<setProductData ruleid="vendordata" field="vendor">
      <getOid>.1.3.6.1.4.1.6302.2.1.1.1.0</getOid>
	</setProductData>

 		<!--model -->
    <suppressRule ruleid="stompmib2model" suppressid="core/mib2data/modeldata"/>
    <setProductData ruleid="modeldata" field="model">
      <getOid>.1.3.6.1.4.1.6302.2.1.1.2.0</getOid>
    </setProductData>
	
	<!--Type -->
	<setProductData ruleid="typedata" field="type">DC Rectifier</setProductData>
  
  	<!-- identControllerFirmwareVersion-->
  	<setProductData ruleid="appversiondata" field="appversion">
      <getOid>.1.3.6.1.4.1.6302.2.1.1.3.0</getOid>
    </setProductData>
    
    <!-- Set Software/Firmware Version -->
    <setProductData ruleid="bootversion" field="bootversion">
      <getOid>.1.3.6.1.4.1.6302.2.1.1.3.0</getOid>
    </setProductData>

	<suppressRule ruleid="stompmib2label" suppressid="core/mib2datalabel/labeldata"/>
	<setDefaultLabel ruleid="labeldata">
      <getOid>.1.3.6.1.4.1.6302.2.1.1.4.0</getOid>
	</setDefaultLabel>    

    <setLocationData ruleid="loc" field="LOCATION">
      <getOid>.1.3.6.1.2.1.1.6.0</getOid>
    </setLocationData>

  </device>


  <device deviceid="acupower">
    <oidMustExist ruleid="acupowerdetect" oid=".1.3.6.1.4.1.6302.2.1.2" istree="yes"/>
	<valueMap ruleid="delete_null_sensor">
		  <!-- omit sensors returning bad value -->
		  <valueIn>2147483647</valueIn><valueOut>[false]</valueOut>
		  <!-- include the rest -->
		  <valueIn>*</valueIn><valueOut>[true]</valueOut>
	</valueMap>

	<!--System-->
		<!--System Status -->
    <stateSensor ruleid="systemStatus">
      <type>state</type>
      <sensorId>SYSTEM_STATUS</sensorId>
      <value>
        <mapValue mapid="systemStatus-vm">
          <getOid>.1.3.6.1.4.1.6302.2.1.2.1.0</getOid>
        </mapValue>
      </value>
      <label>System Status</label>
      <enum>systemStatus-em</enum>
      <sensorSet>system</sensorSet>
    </stateSensor>

    <valueMap ruleid="systemStatus-vm">
      <valueIn>1</valueIn><valueOut>0</valueOut>
      <valueIn>2</valueIn><valueOut>1</valueOut>
      <valueIn>3</valueIn><valueOut>2</valueOut>
      <valueIn>4</valueIn><valueOut>3</valueOut>
      <valueIn>5</valueIn><valueOut>4</valueOut>
      <valueIn>6</valueIn><valueOut>5</valueOut>
      <valueIn>7</valueIn><valueOut>6</valueOut>
      <valueIn>8</valueIn><valueOut>7</valueOut>
      <valueIn>9</valueIn><valueOut>8</valueOut>
      <valueIn>10</valueIn><valueOut>9</valueOut>
      <valueIn>*</valueIn><valueOut>0</valueOut>
    </valueMap>

    <enumMap ruleid="systemStatus-em">
      <label>Unknown</label>
      <label>Normal</label>
      <label>Warning</label>
      <label>Minor - A3</label>
      <label>Major - A3</label>
      <label>Critical - CA</label>
      <label>Unmanaged</label>
      <label>Restricted</label>
      <label>Testing</label>
      <label>Disabled</label>
    </enumMap>
    
    <!-- System Voltage -->
    <numSensor ruleid="systemVoltage">
    	<rowTest>
			 <mapValue mapid="delete_null_sensor">
				<getOid>.1.3.6.1.4.1.6302.2.1.2.2.0</getOid>
			 </mapValue>
		  </rowTest>
      <type>voltage</type>
      <sensorId>SYSTEM_VOLTAGE</sensorId>
      <value>
        <mult>
          <op>
            <getOid>.1.3.6.1.4.1.6302.2.1.2.2.0</getOid>
          </op>
          <op>0.001</op>
        </mult>
      </value>
	  <valueInc>0.001</valueInc>
      <label>System Voltage</label>
      <sensorSet>system</sensorSet>
    </numSensor>
    
    <!-- System Current -->
    <numSensor ruleid="systemCurrent">
    	<rowTest>
			 <mapValue mapid="delete_null_sensor">
				<getOid>.1.3.6.1.4.1.6302.2.1.2.3.0</getOid>
			 </mapValue>
		  </rowTest>
      <type>amperage</type>
      <sensorId>SYSTEM_CURRENT</sensorId>
      <value>
        <mult>
          <op>
            <getOid>.1.3.6.1.4.1.6302.2.1.2.3.0</getOid>
          </op>
          <op>0.001</op>
        </mult>
      </value>
	  <valueInc>0.001</valueInc>
      <label>System Current</label>
      <sensorSet>system</sensorSet>
    </numSensor>
    
    <!-- System Used Capacity -->
    <numSensor ruleid="systemUsedCapacity">
    	<rowTest>
			 <mapValue mapid="delete_null_sensor">
				<getOid>.1.3.6.1.4.1.6302.2.1.2.4.0</getOid>
			 </mapValue>
		  </rowTest>
      <type>pctofcapacity</type>
      <sensorId>SYSTEM_USED_CAPACITY</sensorId>
      <value>
           <getOid>.1.3.6.1.4.1.6302.2.1.2.4.0</getOid>
      </value>
      <label>System Used Capacity</label>
      <sensorSet>system</sensorSet>
    </numSensor>
    
    
    <!-- ps Battery Voltage -->
