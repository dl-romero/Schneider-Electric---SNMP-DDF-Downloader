	<valueMap ruleid="temp_type_vm">
            <valueIn>0</valueIn><valueOut>0</valueOut>
            <valueIn>1</valueIn><valueOut>1</valueOut>
            <valueIn>2</valueIn><valueOut>2</valueOut>
			<valueIn>*</valueIn><valueOut>3</valueOut>
    </valueMap>
    <enumMap ruleid="temp_type_em">
            <label>None</label>
            <label>Ambient</label>
            <label>Battery</label>
			<label>Unknown</label>
    </enumMap>
	
	<alarmFlags2 ruleid="temperature_alarm_status" index=".1.3.6.1.4.1.6302.2.1.2.7.3.1.5">
      <value>
        <getRowOid>.1.3.6.1.4.1.6302.2.1.2.7.3.1.5</getRowOid>
      </value>
      <alarmFlagsTest match="0">
        <name>
          <activeText>Temperature Probe High - Alarm Status Condition Present</activeText>
          <inactiveText>Temperature Probe High - Alarm Status Condition Cleared</inactiveText>
        </name>
        <severity>warning</severity>
        <category>system</category>
        <description>
          <activeText>Temperature Probe High - Alarm Status Condition Present</activeText>
          <inactiveText>Temperature Probe High - Alarm Status Condition Cleared</inactiveText>
        </description>
      </alarmFlagsTest>
	  <alarmFlagsTest match="1">
        <name>
          <activeText>Temperature Probe Low - Alarm Status Condition Present</activeText>
          <inactiveText>Temperature Probe Low - Alarm Status Condition Cleared</inactiveText>
        </name>
        <severity>warning</severity>
        <category>system</category>
        <description>
          <activeText>Temperature Probe Low - Alarm Status Condition Present</activeText>
          <inactiveText>Temperature Probe Low - Alarm Status Condition Cleared</inactiveText>
        </description>
      </alarmFlagsTest>
	  <alarmFlagsTest match="2">
        <name>
          <activeText>Temperature Probe Failure - Alarm Status Condition Present</activeText>
          <inactiveText>Temperature Probe Failure - Alarm Status Condition Cleared</inactiveText>
        </name>
        <severity>critical</severity>
        <category>system</category>
        <description>
          <activeText>Temperature Probe Failure - Alarm Status Condition Present</activeText>
          <inactiveText>Temperature Probe Failure - Alarm Status Condition Cleared</inactiveText>
        </description>
      </alarmFlagsTest>
    </alarmFlags2>
    
