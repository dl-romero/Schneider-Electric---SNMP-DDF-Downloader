	<numSensor ruleid="temperature_measurement" index=".1.3.6.1.4.1.6302.2.1.2.7.3.1.2">
      <type>temp</type>
      <sensorId>_TEMPERATURE_MEASUREMENT<rowIndex/></sensorId>
