    </enumMap>

    <!--
        The precision is different for the string value vs. the int value
        For example, -99.9 is the String value, -100 is the int value. 
        Some sensatronics devices respond with -100 and some with -1000
    -->
    <valueMap ruleid="validTemp-vm">
      <valueIn>-99.9</valueIn><valueOut>[false]</valueOut>
      <valueIn>-100</valueIn><valueOut>[false]</valueOut>
      <valueIn>-999.9</valueIn><valueOut>[false]</valueOut>
      <valueIn>-1000</valueIn><valueOut>[false]</valueOut>
      <valueIn>*</valueIn><valueOut>[true]</valueOut>
    </valueMap>

    <valueMap ruleid="convert-temp-vm">
      <!-- Document says value is 0 or 1. -->
      <!-- Walk of live device has F/C. So try both  -->
      <!-- Return value is [true] if conversion needs to be done, [false] otherwise   -->
      <valueIn>F</valueIn>  <valueOut>[true]</valueOut>
      <valueIn>C</valueIn>  <valueOut>[false]</valueOut>
      <valueIn>0</valueIn>  <valueOut>[false]</valueOut>
      <valueIn>1</valueIn>  <valueOut>[true]</valueOut>
      <valueIn>*</valueIn>  <valueOut>[false]</valueOut>
      </valueMap>
  </device>
