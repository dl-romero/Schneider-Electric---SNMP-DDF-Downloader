<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="apc_ddf_1_6.xsl"?>
<APC_DDF
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://localhost/ddf_schema/apc_ddf_1_6.xsd"
	ddfid="belden_aehc"
	ddfname="Belden AEHC"
	ddfversion="2"
	canremove="yes">
	<schemaVersion>1.6</schemaVersion>
	<device deviceid="belden_aehc">
		<oidMustExist ruleid="belden_aehc_exists" oid=".1.3.6.1.4.1.29414" istree="yes"/>
		<setProductData ruleid="vendordata" field="vendor">Belden</setProductData>
		<setProductData ruleid="typedata" field="type">Environmental Monitor</setProductData>

		<setProductData ruleid="modeldata" field="model">
			<getOid>.1.3.6.1.4.1.29414.1.1.1.0</getOid>
		</setProductData>
		<setProductData ruleid="firmwaredata" field="appversion">
			<getOid>.1.3.6.1.4.1.29414.1.1.2.0</getOid>
		</setProductData>

		<setDefaultLabel>
			<getOid>.1.3.6.1.4.1.29414.1.1.3.0</getOid>
		</setDefaultLabel>
		
		<numSensor ruleid="dewpointsensorhumidity">
			<type>humidity</type>
			<units/>
			<sensorId>_DEWPOINTSENSORHUMIDITY</sensorId>
			<value>
				<op>
					<getOid>.1.3.6.1.4.1.29414.1.3.1.7.1</getOid>
				</op>
			</value>
			<label>dewPointSensorHumidity</label>
			<sensorSet>Generic</sensorSet>
		</numSensor>
		<numSensor ruleid="dewpointsensortempc">
			<type>temp</type>
			<units/>
			<sensorId>_DEWPOINTSENSORTEMPC</sensorId>
			<value>
				<op>
					<getOid>.1.3.6.1.4.1.29414.1.3.1.5.1</getOid>
				</op>
			</value>
			<label>dewPointSensorTempC</label>
			<sensorSet>Generic</sensorSet>
		</numSensor>
		<numSensor ruleid="fancontrolcfm">
			<type>num</type>
			<units>feet3permin</units>
			<sensorId>_FANCONTROLCFM</sensorId>
			<value>
				<op>
					<getOid>.1.3.6.1.4.1.29414.1.4.1.16.1</getOid>
				</op>
			</value>
			<label>fancontrolCFM</label>
			<sensorSet>Generic</sensorSet>
		</numSensor>
		<numSensor ruleid="fancontrolcapacitya">
			<type>pctofcapacity</type>
			<units/>
			<sensorId>_FANCONTROLCAPACITYA</sensorId>
			<value>
				<op>
					<getOid>.1.3.6.1.4.1.29414.1.4.1.11.1</getOid>
				</op>
			</value>
			<label>fancontrolCapacityA</label>
			<sensorSet>Generic</sensorSet>
		</numSensor>
		<numSensor ruleid="fancontrolcapacityb">
			<type>pctofcapacity</type>
			<units/>
			<sensorId>_FANCONTROLCAPACITYB</sensorId>
			<value>
				<op>
					<getOid>.1.3.6.1.4.1.29414.1.4.1.12.1</getOid>
				</op>
			</value>
			<label>fancontrolCapacityB</label>
			<sensorSet>Generic</sensorSet>
		</numSensor>
		<numSensor ruleid="fancontrolpressdiff">
			<type>airdiffpressure</type>
			<units/>
			<sensorId>_FANCONTROLPRESSDIFF</sensorId>
			<value>
				<op>
					<getOid>.1.3.6.1.4.1.29414.1.4.1.14.1</getOid>
				</op>
			</value>
			<label>fancontrolPressDiff</label>
			<sensorSet>Generic</sensorSet>
		</numSensor>
		<numSensor ruleid="fancontrolrpmfana">
			<type>num</type>
			<units>rpm</units>
			<sensorId>_FANCONTROLRPMFANA</sensorId>
			<value>
				<op>
					<getOid>.1.3.6.1.4.1.29414.1.4.1.9.1</getOid>
				</op>
			</value>
			<label>fancontrolRpmFanA</label>
			<sensorSet>Generic</sensorSet>
		</numSensor>
		<numSensor ruleid="fancontrolrpmfanb">
			<type>num</type>
			<units>rpm</units>
			<sensorId>_FANCONTROLRPMFANB</sensorId>
			<value>
				<op>
					<getOid>.1.3.6.1.4.1.29414.1.4.1.10.1</getOid>
				</op>
			</value>
			<label>fancontrolRpmFanB</label>
			<sensorSet>Generic</sensorSet>
		</numSensor>
		<numSensor ruleid="fancontrolsetpoint">
			<type>num</type>
			<units/>
			<sensorId>_FANCONTROLSETPOINT</sensorId>
			<value>
				<op>
					<getOid>.1.3.6.1.4.1.29414.1.4.1.15.1</getOid>
				</op>
			</value>
			<label>fancontrolSetPoint</label>
			<sensorSet>Generic</sensorSet>
		</numSensor>
		<numSensor ruleid="fancontroltempcreturna">
			<type>temp</type>
			<units/>
			<sensorId>_FANCONTROLTEMPCRETURNA</sensorId>
			<value>
				<op>
					<getOid>.1.3.6.1.4.1.29414.1.4.1.5.1</getOid>
				</op>
			</value>
			<label>fancontrolTempCreturnA</label>
			<sensorSet>Generic</sensorSet>
		</numSensor>
		<numSensor ruleid="fancontroltempcreturnb">
			<type>temp</type>
			<units/>
			<sensorId>_FANCONTROLTEMPCRETURNB</sensorId>
			<value>
				<op>
					<getOid>.1.3.6.1.4.1.29414.1.4.1.7.1</getOid>
				</op>
			</value>
			<label>fancontrolTempCreturnB</label>
			<sensorSet>Generic</sensorSet>
		</numSensor>

		<stateSensor ruleid="fancontrol_press_err">
			<type>state</type>
			<sensorId>_FANCONTROL_PRESS_ERR</sensorId>
			<value>
				<mapValue mapid="yes_no_valuemap">
					<getOid>.1.3.6.1.4.1.29414.1.4.1.13.1</getOid>
				</mapValue>
			</value>
			<label>Fan control press error</label>
			<enum>yes_no_enum</enum>
			<sensorSet>Generic</sensorSet>
		</stateSensor>
		<valueMap ruleid="yes_no_valuemap">
			<valueIn>1</valueIn>
			<valueOut>1</valueOut>
			<valueIn>*</valueIn>
			<valueOut>0</valueOut>
		</valueMap>
		<enumMap ruleid="yes_no_enum">
			<label>No</label>
			<label>Yes</label>
		</enumMap>

	</device>

</APC_DDF>
