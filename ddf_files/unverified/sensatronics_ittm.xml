   	 <numSensor ruleid="sensor1DataStr">
      <type>temp</type>
      <sensorId>SENSOR_1_DATA_STR</sensorId>
         <value>   
         	<if><!-- Fahrenheit -->
          	<op>
            	<eq>
              	<op><mapValue mapid="whattype"><getOid>.1.3.6.1.4.1.16174.1.1.1.2.3.1.0</getOid></mapValue></op>
    						<op>1</op> <!-- Temperature scale-->
              </eq>
            </op>
              <op><!-- true fahrenheit -->
               	<div>
               		<op>
               			<sub>
               				<op><getOid>.1.3.6.1.4.1.16174.1.1.1.3.1.2.0</getOid></op>	
               				<op>32</op>
                		</sub>
               		</op>
               		<op>1.8</op>
               	</div>	        
            </op><!-- end fahrenheit -->  					 
            <op>
            	<if><!-- Celsius -->
               	<op>
                	<eq>
                		<op><mapValue mapid="whattype"><getOid>.1.3.6.1.4.1.16174.1.1.1.2.3.1.0</getOid></mapValue></op>
    								<op>0</op> <!-- Temperature scale-->
                	</eq>
               	</op>
               	<op><getOid>.1.3.6.1.4.1.16174.1.1.1.3.1.2.0</getOid></op><!--celsius -->
               	<op><getOid>.1.3.6.1.4.1.16174.1.1.1.3.1.2.0</getOid></op><!-- default celsius -->
            	</if>
            </op><!--default celsius -->
         </if>
      	</value>
       <label>sensor 1 Data Str</label>
       <sensorSet>Sensor Data</sensorSet>
    </numSensor>	 
    
