    <!-- upsIdent -->
  <device deviceid="jema_toshiba">
    <oidMustExist ruleid="mib2objidtest" oid=".1.3.6.1.4.1.4550.1.1.1.1.0"/>


    <!-- Replace MIB2 Model with Model from UPS MIB -->
    <suppressRule ruleid="stompmib2model" suppressid="core/mib2data/modeldata"/>
    <suppressRule suppressid= "upsmib/upsmibdiscdata/modeldata"/> <!-- supress upsmib model -->
    <setProductData ruleid="modeldata" field="model">Toshiba</setProductData>
    
    <suppressRule ruleid="stompmib2type" suppressid="core/mib2data/typedata"/>
    <setProductData ruleid="typedata" field="type">UPS</setProductData>
    
    
    <!-- Set Product Version -->
    </device>  
    
   
