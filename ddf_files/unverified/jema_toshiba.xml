      <sensorId>BATT_VOLTAGE</sensorId>
      <value>
        <mult>
          <op>
            <getOid>.1.3.6.1.4.1.4550.1.1.2.5.0</getOid>
          </op>
          <op>0.1</op>
        </mult>
      </value>
      <label>Battery Voltage</label>
      <sensorSet>UPS Battery Status</sensorSet>
    </numSensor>

