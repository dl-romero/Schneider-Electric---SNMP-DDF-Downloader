      </value>
      <label>Battery Time Remaining</label>
      <sensorSet>UPS Battery Status</sensorSet>
    </numSensor>

    <!-- upsEstimatedChargeRemaining -->
    <numSensor ruleid="jemaUpsEstimatedChargeRemaining">
