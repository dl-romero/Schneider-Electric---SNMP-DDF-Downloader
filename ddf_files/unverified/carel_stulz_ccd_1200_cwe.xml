    <!-- temp-setpointSensor -->
    <numSensor ruleid="temp-setpointSensor">
      <type>temp</type>
      <sensorId>TEMP_SETPOINT_SENSOR</sensorId>
      <value>
        <!-- Farenheit to Celcius -->
        <div>
          <op>
            <sub>
              <op>
                <!-- scale by .1 -->
                <mult>
                  <op>0.1</op>
                  <op>
                    <getOid>.1.3.6.1.4.1.9839.2.1.2.1.0</getOid>
                  </op>
                </mult>
              </op>
              <op>32</op>
            </sub>
          </op>
          <op>1.8</op>
        </div>
      </value>
      <label>Temperature Setpoint</label>
      <sensorSet>analogObjects</sensorSet>
    </numSensor>

    <!-- hum-setpointSensor -->
    <numSensor ruleid="hum-setpointSensor">
      <type>humidity</type>
      <sensorId>HUM_SETPOINT_SENSOR</sensorId>
      <value>
        <mult>
          <op>0.1</op>
          <op>
            <getOid>.1.3.6.1.4.1.9839.2.1.2.2.0</getOid>
          </op>
        </mult>
      </value>
