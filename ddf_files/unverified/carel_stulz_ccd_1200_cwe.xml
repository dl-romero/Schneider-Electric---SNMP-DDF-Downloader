      <sensorSet>analogObjects</sensorSet>
    </numSensor>

    <!-- ret-t-dispSensor -->
    <numSensor ruleid="ret-t-dispSensor">
      <type>temp</type>
      <sensorId>RET_T_DISP_SENSOR</sensorId>
      <value>
        <!-- Farenheit to Celcius -->
        <div>
          <op>
            <sub>
              <op>
                <!-- 0.1x Scalar -->
                <mult>
                  <op>
                    <getOid>.1.3.6.1.4.1.9839.2.1.2.3.0</getOid>
                  </op>
                  <op>0.1</op>
                </mult>
              </op>
              <op>32</op>
            </sub>
          </op>
          <op>1.8</op>
        </div>
      </value>
      <label>Return Temperature</label>
