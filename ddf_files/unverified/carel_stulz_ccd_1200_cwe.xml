    <!-- pCOId1 Status -->
    <stateSensor ruleid="pCOId1-Status">
      <type>state</type>
      <sensorId>PCOID1_STATUS</sensorId>
      <value>
        <getOid>.1.3.6.1.4.1.9839.2.0.10.1.0</getOid>
      </value>
      <label>pCOId1 Status </label>
      <enum>status-em</enum>
      <sensorSet>pcoId1</sensorSet>
    </stateSensor>

    <enumMap ruleid="status-em">
      <label>Offline</label>
      <label>Init</label>
      <label>Online</label>
    </enumMap>

