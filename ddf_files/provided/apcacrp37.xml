<?xml version="1.0"?>
<!-- APC NetworkAir In Row ACRP37 Cooling product DDF -->
<APC_DDF xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://localhost/ddf_schema/apc_ddf_1_6.xsd"
	ddfid="apcacrp37"
	ddfname="APC NetworkAIR RP37"
		ddfversion="16">
    <schemaVersion>1.6</schemaVersion>
    <!-- APC NetworkAir In Row ACRP 37kW Cooling product specific functionality -->

    <!-- ACRP37_Group Section -->

    <device deviceid="group_acrp37">
	   <oidMustExist ruleid="groupcooloutputoid" oid=".1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.8.0"/>
	   
	   <!-- Group Cool Output in Watts (originally in 1/10th of kW) -->
	   <numSensor ruleid="cooloutput">
		  <type>powerW/cooloutput</type>
		  <sensorId>ACRP37_GRP_COOL_OUTPUT</sensorId>
		  <value>
			 <round increment="0.1">
				<mult>
				    <op>
					   <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.8.0</getOid>
				    </op>
				    <op>100.0</op>
				</mult>
			 </round>
		  </value>
		  <label>Group Cool Output</label>
		  <sensorSet>Group Status</sensorSet>
	   </numSensor>
	   
	   <!-- Group Cool Demand in Watts (originally in 1/10ths of kW) -->
	   <numSensor ruleid="cooldemand">
		  <type>powerW</type>
		  <sensorId>ACRP37_GRP_COOL_DEMAND</sensorId>
		  <value>
			 <round increment="0.1">
				<mult>
				    <op>
					   <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.7.0</getOid>
				    </op>
				    <op>100</op>
				</mult>
			 </round>
		  </value>
		  <label>Group Cool Demand</label>
		  <sensorSet>Group Status</sensorSet>
	   </numSensor>
	   
	   <!-- Group Volume Airflow in liters/min (Originally in liters/sec) -->
	   <numSensor ruleid="groupvolumeairflow">
		  <rowTest>
			 <mapValue mapid="delete_null_sensor">
				<getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.2.0</getOid>
			 </mapValue>
		  </rowTest>
		  <type>volairflow</type>
		  <sensorId>ACRP37_GRP_VOLUME_AIR_FLOW</sensorId>
		  <value>
			 <mult>
				<op>
				    <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.2.0</getOid>
				</op>
				<op>60</op>
			 </mult>
		  </value>
		  <label>Group Volume Air Flow</label>
		  <sensorSet>Group Status</sensorSet>
	   </numSensor>
		<!--
	   <numSensor ruleid="groupvolumeairflowsec">
		  <rowTest>
			 <mapValue mapid="delete_null_sensor">
				<getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.2.0</getOid>
			 </mapValue>
		  </rowTest>
		  <type>volairflowsec</type>
		  <sensorId>ACRP37_GRP_VOLUME_AIR_FLOW_SEC</sensorId>
		  <value>
			<getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.2.0</getOid>
		  </value>
		  <label>Group Volume Air Flow (per second)</label>
		  <sensorSet>Group Status</sensorSet>
	   </numSensor>
		-->
	   
	   <!-- Group Maximum Rack Inlet Temperature in degrees C -->
	   <numSensor ruleid="grpmaxtempofrackinlet">
		 <rowTest>
		   <or>
			 <op>
			   <neq>
				 <op><getOid default="-1">.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.3.0</getOid></op>
				 <op>-1</op>
			   </neq>
			 </op>
			  <op>
			   <neq>
				 <op><getOid default="-1">.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.4.0</getOid></op>
				 <op>-1</op>
			   </neq>
			 </op>
		   </or>
		 </rowTest>
		  <type>temp</type>
		  <sensorId>MAX_RACK_INLET_TEMP</sensorId>
		  <value>
			 <round increment="0.1">
				<mult>
				    <op>
					   <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.4.0</getOid>
				    </op>
				    <op>0.1</op>
				</mult>
			 </round>
		  </value>
		  <label>Group Rack Inlet Temperature (Max)</label>
		  <sensorSet>Group Status</sensorSet>
	   </numSensor>
	   
	   <!-- Group Minimum Rack Inlet Temperature in degrees C -->
	   <numSensor ruleid="grpmintempofrackinlet">
		 <rowTest>
		   <or>
			 <op>
			   <neq>
				 <op><getOid default="-1">.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.5.0</getOid></op>
				 <op>-1</op>
			   </neq>
			 </op>
			  <op>
			   <neq>
				 <op><getOid default="-1">.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.6.0</getOid></op>
				 <op>-1</op>
			   </neq>
			 </op>
		   </or>
		 </rowTest>
		  <type>temp</type>
		  <sensorId>MIN_RACK_INLET_TEMP</sensorId>
		  <value>
			 <round increment="0.1">
				<mult>
				    <op>
					   <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.6.0</getOid>
				    </op>
				    <op>0.1</op>
				</mult>
			 </round>
		  </value>
		  <label>Group Rack Inlet Temperature (Min)</label>
		  <sensorSet>Group Status</sensorSet>
	   </numSensor>
	   
	   <!-- Group Humidify Demand in % -->
	   <numSensor ruleid="grphumidifydemand">
		  <rowTest>
			 <mapValue mapid="delete_null_sensor">
				<getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.9.0</getOid>
			 </mapValue>
		  </rowTest>
		  <type>pctofcapacity/pctcoolingcap/humdemand</type>
		  <sensorId>HUMIDIFY_DEMAND</sensorId>
		  <value>
			 <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.9.0</getOid>
		  </value>
		  <label>Group Humidify Demand</label>
		  <sensorSet>Group Status</sensorSet>
	   </numSensor>
	   
	   <!-- Group Humidify Output in % -->
	   <numSensor ruleid="grphumidifyoutput">
		  <rowTest>
			 <mapValue mapid="delete_null_sensor">
				<getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.10.0</getOid>
			 </mapValue>
		  </rowTest>
		  <type>pctofcapacity/pctcoolingcap</type>
		  <sensorId>HUMIDIFY_OUTPUT</sensorId>
		  <value>
			 <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.10.0</getOid>
		  </value>
		  <label>Group Humidify Output</label>
		  <sensorSet>Group Status</sensorSet>
	   </numSensor>
	   
	   <!-- Group Dehumidify Demand in % -->
	   <numSensor ruleid="groupdehumidifydemand">
		  <rowTest>
			 <mapValue mapid="delete_null_sensor">
				<getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.11.0</getOid>
			 </mapValue>
		  </rowTest>
		  <type>pctofcapacity/pctcoolingcap</type>
		  <sensorId>ACRP37_GRP_DEHUMIDIFY_DEMAND</sensorId>
		  <value>
			 <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.11.0</getOid>
		  </value>
		  <label>Group Dehumidify Demand</label>
		  <sensorSet>Group Status</sensorSet>
	   </numSensor>
	   
	   <!-- Group Dehumidify Output in % -->
	   <numSensor ruleid="groupdehumidifyoutput">
		  <rowTest>
			 <mapValue mapid="delete_null_sensor">
				<getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.12.0</getOid>
			 </mapValue>
		  </rowTest>
		  <type>pctofcapacity/pctcoolingcap</type>
		  <sensorId>ACRP37_GRP_DEHUMIDIFY_OUTPUT</sensorId>
		  <value>
			 <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.12.0</getOid>
		  </value>
		  <label>Group Dehumidify Output</label>
		  <sensorSet>Group Status</sensorSet>
	   </numSensor>
	   
	   <!-- Group Reheat Demand in % -->
	   <numSensor ruleid="groupreheatdemand">
		  <rowTest>
			 <mapValue mapid="delete_null_sensor">
				<getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.13.0</getOid>
			 </mapValue>
		  </rowTest>
		  <type>pctofcapacity/pctcoolingcap</type>
		  <sensorId>ACRP37_GRP_REHEAT_DEMAND</sensorId>
		  <value>
			 <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.13.0</getOid>
		  </value>
		  <label>Group Reheat Demand</label>
		  <sensorSet>Group Status</sensorSet>
	   </numSensor>
	   
	   <!-- Group Reheat Output in % -->
	   <numSensor ruleid="grpreheatoutput">
		  <rowTest>
			 <mapValue mapid="delete_null_sensor">
				<getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.14.0</getOid>
			 </mapValue>
		  </rowTest>
		  <type>pctofcapacity/pctcoolingcap</type>
		  <sensorId>ACRP37_GRP_REHEAT_OUTPUT</sensorId>
		  <value>
			 <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.14.0</getOid>
		  </value>
		  <label>Group Reheat Output</label>
		  <sensorSet>Group Status</sensorSet>
	   </numSensor>

	<stateSensor ruleid="activeFlowcontrolStatus">
	    <rowTest>
		  <mapValue mapid="delete_null_sensor">
			 <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.15.0</getOid>
		  </mapValue>
	   </rowTest>
	    <type>state</type>
	    <sensorId>ACRP25_GRP_FLOW_CONTROL</sensorId>
	    <value>
			<mapValue mapid="flowControlStatus-vm">
			<getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.15.0</getOid>
			</mapValue>
	    </value>
		<enum>flowControlStatus-em</enum>
	    <label>Active Flow Control Status</label>
	    <sensorSet>Group Status</sensorSet>
	</stateSensor>
	<valueMap ruleid="flowControlStatus-vm">
		<valueIn>1</valueIn><valueOut>1</valueOut>
		<valueIn>2</valueIn><valueOut>2</valueOut>
		<valueIn>3</valueIn><valueOut>3</valueOut>
		<valueIn>4</valueIn><valueOut>4</valueOut>
		<valueIn>*</valueIn><valueOut>0</valueOut>
	</valueMap>
	<enumMap ruleid="flowControlStatus-em">
		<label>Unknown</label>
		<label>Under</label>
		<label>OK</label>
		<label>Over</label>
		<label>N/A</label>
	</enumMap>

	<numSensor ruleid="dewPointTempMetric">
	    <rowTest>
		  <mapValue mapid="delete_null_sensor">
			 <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.17.0</getOid>
		  </mapValue>
	   </rowTest>
	    <type>temp</type>
	    <sensorId>ACRP25_GRP_DEWPOINT</sensorId>
	    <value>
			<mult>
			<op><getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.1.17.0</getOid></op>
			<op>0.1</op>
			</mult>
	    </value>
		<valueInc>0.1</valueInc>
	    <label>Dew Point Temperature</label>
	    <sensorSet>Group Status</sensorSet>
	</numSensor>

	<stateSensor ruleid="activeFlowControlBias">
	    <rowTest>
		  <mapValue mapid="delete_null_sensor">
			 <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.2.12.0</getOid>
		  </mapValue>
	   </rowTest>
	    <type>state</type>
	    <sensorId>ACRP25_GRP_FLOW_CONTROL_BIAS</sensorId>
	    <value>
			<mapValue mapid="flowControlBias-vm">
			<getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.2.12.0</getOid>
			</mapValue>
	    </value>
		<enum>flowControlBias-em</enum>
	    <label>Active Flow Control Bias</label>
	    <sensorSet>Group Status</sensorSet>
	</stateSensor>
	<valueMap ruleid="flowControlBias-vm">
		<valueIn>1</valueIn><valueOut>1</valueOut>
		<valueIn>2</valueIn><valueOut>2</valueOut>
		<valueIn>3</valueIn><valueOut>3</valueOut>
		<valueIn>4</valueIn><valueOut>4</valueOut>
		<valueIn>5</valueIn><valueOut>5</valueOut>
		<valueIn>*</valueIn><valueOut>0</valueOut>
	</valueMap>
	<enumMap ruleid="flowControlBias-em">
		<label>Unknown</label>
		<label>Positive</label>
		<label>Slightly Positive</label>
		<label>Zero</label>
		<label>Slightly Negative</label>
		<label>Negative</label>
	</enumMap>

	<stateSensor ruleid="configurationType">
	    <rowTest>
		  <mapValue mapid="delete_null_sensor">
			 <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.3.2.0</getOid>
		  </mapValue>
	   </rowTest>
	    <type>state</type>
	    <sensorId>ACRP25_GRP_CONFIG_TYPE</sensorId>
	    <value>
			<mapValue mapid="configType-vm">
			<getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.3.2.0</getOid>
			</mapValue>
	    </value>
		<enum>configType-em</enum>
	    <label>Configuration Type</label>
	    <sensorSet>Group Status</sensorSet>
	</stateSensor>
	<valueMap ruleid="configType-vm">
		<valueIn>1</valueIn><valueOut>1</valueOut>
		<valueIn>2</valueIn><valueOut>2</valueOut>
		<valueIn>3</valueIn><valueOut>3</valueOut>
		<valueIn>4</valueIn><valueOut>4</valueOut>
		<valueIn>*</valueIn><valueOut>0</valueOut>
	</valueMap>
	<enumMap ruleid="configType-em">
		<label>Unknown</label>
		<label>RACS</label>
		<label>HACS</label>
		<label>InRow</label>
		<label>CACS</label>
	</enumMap>

	<numSensor ruleid="numActiveFlowControllers">
	    <rowTest>
		  <mapValue mapid="delete_null_sensor">
			 <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.3.16.0</getOid>
		  </mapValue>
	   </rowTest>
	    <type>num</type>
	    <sensorId>ACRP25_GRP_NUM_ACTIVE_FLOW_CTRLRS</sensorId>
	    <value>
			<mult>
			<op><getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.3.16.0</getOid></op>
			<op>1</op>
			</mult>
	    </value>
		<valueInc>1</valueInc>
	    <label>Number of Active Flow Controllers</label>
	    <sensorSet>Group Status</sensorSet>
	</numSensor>

	<stateSensor ruleid="lampTest">
	    <rowTest>
		  <mapValue mapid="delete_null_sensor">
			 <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.3.17.0</getOid>
		  </mapValue>
	   </rowTest>
	    <type>state</type>
	    <sensorId>ACRP25_GRP_LAMP_TEST</sensorId>
	    <value>
			<mapValue mapid="lampTest-vm">
			<getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.3.17.0</getOid>
			</mapValue>
	    </value>
		<enum>lampTest-em</enum>
	    <label>Active Flow Controller Lamp Test</label>
	    <sensorSet>Group Status</sensorSet>
	</stateSensor>
	<valueMap ruleid="lampTest-vm">
		<valueIn>1</valueIn><valueOut>1</valueOut>
		<valueIn>2</valueIn><valueOut>2</valueOut>
		<valueIn>*</valueIn><valueOut>0</valueOut>
	</valueMap>
	<enumMap ruleid="lampTest-em">
		<label>Unknown</label>
		<label>Off</label>
		<label>On</label>
	</enumMap>

	<numSensor ruleid="altitudeMetric">
	    <rowTest>
		  <mapValue mapid="delete_null_sensor">
			 <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.3.19.0</getOid>
		  </mapValue>
	   </rowTest>
	    <type>num</type>
		<units>meters</units>
	    <sensorId>ACRP25_GRP_ALTITUDE</sensorId>
	    <value>
			<mult>
			<op><getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.1.3.19.0</getOid></op>
			<op>1</op>
			</mult>
	    </value>
		<valueInc>1</valueInc>
	    <label>Altitude</label>
	    <sensorSet>Group Status</sensorSet>
	</numSensor>

	   <valueMap ruleid="delete_null_sensor">
		  <!-- omit sensors returning -1 -->
		  <valueIn>-1</valueIn><valueOut>[false]</valueOut>
		  <!-- include the rest -->
		  <valueIn>*</valueIn><valueOut>[true]</valueOut>
	   </valueMap>
    </device>
    
    
    <!-- ACRP37_Unit Section -->

    <device deviceid="unit_acrp37">
	   <oidMustExist ruleid="unitcooloutputoid" oid=".1.3.6.1.4.1.318.1.1.13.3.3.2.2.2.14.0"/>
	   
	   <!-- Unit Cool Output in Watts (Originally in 1/10th of kW)-->
	   <numSensor ruleid="cooloutput">
		  <rowTest>
			 <mapValue mapid="apcacrp37/group_acrp37/delete_null_sensor">
				<getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.2.2.14.0</getOid>
			 </mapValue>
		  </rowTest>
		  <type>powerW/unitcooloutput</type>
		  <sensorId>ACRP37_UNIT_COOL_OUTPUT</sensorId>
		  <value>
			 <round increment="0.1">
				<mult>
				    <op>
					   <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.2.2.14.0</getOid>
				    </op>
				    <op>100.0</op>
				</mult>
			 </round>
		  </value>
		  <label>Unit Cool Output</label>
		  <sensorSet>Unit Status Overview</sensorSet>
	   </numSensor>
	   
	   <!-- Unit Operate Mode -->
	   <stateSensor ruleid="operatemode">
		  <type>state/operatemode</type>
		  <enumid>acopmode</enumid>
		  <sensorId>OPERATE_MODE</sensorId>
		  <value>
			 <mapValue mapid="operatemodemap">
				<getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.2.2.1.0</getOid>
			 </mapValue>
		  </value>
		  <label>Unit Operating Mode</label>
		  <!-- the enumMap will implicitly define enuminc -->
		  <enum>operatemode-enum</enum>
		  <sensorSet>Unit Status Overview</sensorSet>
	   </stateSensor>
	   
	   <!-- Unit Rack Inlet Temperature in degrees C -->
	   <numSensor ruleid="unitmaxtempofrackinlet">
		 <rowTest>
		   <or>
			 <op>
			   <neq>
				 <op><getOid default="-1">.1.3.6.1.4.1.318.1.1.13.3.3.2.2.2.2.0</getOid></op>
				 <op>-1</op>
			   </neq>
			 </op>
			  <op>
			   <neq>
				 <op><getOid default="-1">.1.3.6.1.4.1.318.1.1.13.3.3.2.2.2.3.0</getOid></op>
				 <op>-1</op>
			   </neq>
			 </op>
		   </or>
		 </rowTest>
		  <type>temp</type>
		  <sensorId>RACK_INLET_TEMP</sensorId>
		  <value>
			 <round increment="0.1">
				<mult>
				    <op>
					   <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.2.2.3.0</getOid>
				    </op>
				    <op>0.1</op>
				</mult>
			 </round>
		  </value>
		  <label>Unit Rack Inlet Temperature (Max)</label>
		  <sensorSet>Unit Status Overview</sensorSet>
	   </numSensor>
	   
	   <!-- Unit Supply Air Temperature in degrees C -->
	   <numSensor ruleid="tempofsupplyair">
		 <rowTest>
		   <or>
			 <op>
			   <neq>
				 <op><getOid default="-1">.1.3.6.1.4.1.318.1.1.13.3.3.2.2.2.4.0</getOid></op>
				 <op>-1</op>
			   </neq>
			 </op>
			  <op>
			   <neq>
				 <op><getOid default="-1">.1.3.6.1.4.1.318.1.1.13.3.3.2.2.2.5.0</getOid></op>
				 <op>-1</op>
			   </neq>
			 </op>
		   </or>
		 </rowTest>
		  <type>temp</type>
		  <sensorId>SUPPLY_AIR_TEMP</sensorId>
		  <value>
			 <round increment="0.1">
				<mult>
				    <op>
					   <getOid>.1.3.6.1.4.1.318.1.1.13.3.3.2.2.2.5.0</getOid>
				    </op>
				    <op>0.1</op>
				</mult>
			 </round>
		  </value>
		  <label>Unit Supply Air Temperature</label>
		  <sensorSet>Unit Status Overview</sensorSet>
	   </numSensor>
	   
	   <!-- Unit Return Air Temperature in degrees C -->
	   <numSensor ruleid="tempofreturnair">
		 <rowTest>
		   <or>
			 <op>
			   <neq>
				 <op><getOid default="-1">.1.3.6.1.4.1.318.1.1.13.3.3.2.2.2.6.0</getOid></op>
				 <op>-1</op>
			   </neq>
			 </op>
			  <op>
